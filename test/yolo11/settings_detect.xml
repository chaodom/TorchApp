<?xml version="1.0"?>
<opencv_storage>
  <DetectType>yolo11</DetectType> <!--yolo11, yolo26, rcnn-->
  <Agnostic>1</Agnostic>  <!--NMS时是否类别无关-->
  <ScoreThreshold>0.25</ScoreThreshold>
  <NMSThreshold>0.45</NMSThreshold>
  <IncludeClasses></IncludeClasses>
  <ScaleFill>0</ScaleFill>  <!--预处理时是否直接拉伸-->
  <IsCenter>1</IsCenter>  <!--预处理时图像是否置为中央-->
  <PaddingValue>114</PaddingValue>  <!--预处理时空白区域填充色-->
  <Verify>0</Verify>  <!--若为真,则图片文件夹内须包含annotation.txt-->
  <IOU>0.9</IOU>  <!--核对时的IOU阈值-->
</opencv_storage>
